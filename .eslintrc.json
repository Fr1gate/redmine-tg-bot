{
  "env": {
    "browser": false,
    "es2021": true
  },
  "extends": ["standard-with-typescript", "eslint-config-prettier"],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": [
      "./tsconfig.json"
    ]
  },
  "rules": {
    "indent": [
      0,
      2
    ],
    "@typescript-eslint/restrict-template-expressions": [
      2,
      {
        "allowAny": true
      }
    ]
  }
}
